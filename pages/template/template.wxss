page{
  background: #f5f5f5;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}
 
.nav_left{
  position:fixed;
  top:0;
  left:0;
  display: inline-block;
  width: 25%;
  min-height: 100%;
  background: #f5f5f5;
  text-align: center;

}
.nav_left .nav_left_items{
  height: 30px;
  line-height: 30px;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 14px;
}
.nav_left .nav_left_items.active{
  background: #fff;
}
 
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  flex: 1;
  width: 73%;
  min-height: 100%;
  background: #fff;
  overflow-y: scroll;
  padding-top:10px;
}
.nav_right .nav_right_items{
  text-align: center;
  margin-bottom: 15px;
  float: left;
  width:50%;
  overflow: hidden;
}
.nav_right .nav_right_items image{
  max-width: 100%;
  max-height: 100px;
}
.nav_right .nav_right_items text{
  display: inline-block;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
